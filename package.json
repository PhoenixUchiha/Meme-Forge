{
  "name": "memeforge",
  "version": "1.0.0",
  "description": "High-performance, extensible meme engine for Discord bots and Node.js.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "tsc --noEmit"
  },
  "keywords": [
    "meme",
    "discord",
    "reddit",
    "engine",
    "fetcher",
    "embed",
    "bot"
  ],
  "author": "PhoenixUchiha",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.7"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "^1.3.0"
  }
}